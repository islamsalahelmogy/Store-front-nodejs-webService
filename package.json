{
  "name": "store-front-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/.",
    "dev": "ts-node src/index.ts",
    "watch": "tsc-watch --onSuccess \"node ./build\"",
    "build": "npx tsc",
    "pretest": "npm run lint && npm run prettier && npm run build",
    "migrate": "db-migrate --env test up && db-migrate up",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.{ts,json} --fix",
    "test": "set ENV=test&& db-migrate reset --env test && db-migrate --env test up && jasmine-ts",
    "prettier": "prettier --config .prettierrc.json src/**/*.ts --write",
    "migrate:reset": "db-migrate --env test reset && db-migrate --env dev reset"
  },
  "author": "Islam ELmogy",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0"
  }
}
